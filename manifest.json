{
   "manifest_version" : 3,
   "name" : "Timer",
   "version" : "1.0.0",
   "description" : "hello chrome worlds!",
   "icons": {
    "16" : "timer3.png",
    "48" : "timer3.png",
    "128" : "timer3.png"
   },

   "action" : {
    "default_icon":{
        "16" : "timer3.png",
        "48" : "timer3.png",
        "128" : "timer3.png"
       },

       "default_title" : "Timer action",
       "default_popup" : "popup.html"
    },

    "options_page" : "options.html",
    "permissions" : ["storage" , "alarms" , "notifications"],
    "background" : {
        "service_worker" : "background.js"
    }

   
}